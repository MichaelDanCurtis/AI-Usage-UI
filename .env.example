# Server Configuration
PORT=3001
HOST=localhost

# Monitoring Configuration
# Enable or disable API monitoring (default: true)
MONITORING_ENABLED=true

# Comma-separated list of APIs to monitor (default: all available)
# Options: anthropic, openrouter, github, google, zai, vercel
MONITORED_APIS=anthropic,openrouter,github,google,zai,vercel

# API Keys
# Get your API keys from each provider's dashboard

# Anthropic Claude
# https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenRouter
# https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# GitHub Copilot
# https://github.com/settings/tokens
# Requires 'read:org' and 'read:user' scopes
GITHUB_TOKEN=your_github_token_here
# For enterprise/org-level Copilot metrics (optional)
# Requires PAT with 'manage_billing:copilot' or 'read:enterprise' scope
GITHUB_ENTERPRISE=your_enterprise_slug
GITHUB_ORG=your_org_name

# Google AI (Gemini)
# https://makersuite.google.com/app/apikey
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Z.ai GLM Coding Plan
# https://api.z.ai - API key from Z.ai platform
ZAI_API_KEY=your_zai_api_key_here

# Vercel
# https://vercel.com/account/tokens
VERCEL_TOKEN=your_vercel_token_here

# Warp.dev (macOS only)
# Run ./scripts/extract-warp-token.sh to automatically extract your token
# Or manually get it from: security find-generic-password -s "dev.warp.Warp-Stable" -a "User" -w
# Note: This token expires periodically and will need to be refreshed
WARP_TOKEN=your_warp_firebase_id_token_here

# Cache Configuration
# Time to live for cached data in seconds (default: 300 = 5 minutes)
CACHE_TTL=300

# Logging
# Options: trace, debug, info, warn, error, fatal
LOG_LEVEL=info
